/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0 0; 
  box-sizing: border-box;
} 

.flex{
  display: flex;
}
.flex-wrap{
  flex-wrap: wrap;
}
.w2{
  width:20%;
}
.w3{
  width:30%;
}
.w4{
  width:40%;
}
.w5{
  width: 50%;
}
.w6{
  width:60%;
}
.w7{
  width:70%;
}
.w8{
  width:80%;
}
.w9{
  width:90%;
}
.w10{
  width:100%;
}
.auto{
  margin-left:auto;
  margin-right:auto;
}
.m1{
  margin-top:10rpx;
  margin-bottom: 10rpx;
}
.m2{
  margin-top:20rpx;
  margin-bottom: 20rpx;
}
.m3{
  margin-top:30rpx;
  margin-bottom: 30rpx;
}
.m4{
  margin-top:40rpx;
  margin-bottom: 40rpx;
}
.m5{
  margin-top:50rpx;
  margin-bottom: 50rpx;
}
.m6{
  margin-top:60rpx;
  margin-bottom: 60rpx;
}
.m7{
  margin-top:70rpx;
  margin-bottom: 70rpx;
}
.m8{
  margin-top:80rpx;
  margin-bottom: 80rpx;
}
.center{
  text-align: center;
}


/* table */
.table {
  border:1px solid #dadada;
  border-right: 0;
  border-bottom: 0;
  width: 98%;
}
.tr {
  width: 100%;
  display: flex;
  /* justify-content: space-between; */
}
.th,.td {
  padding: 10px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  /* width:100% */
}
.th {
  font-weight: 400;
  background-color: #dadada;
  text-align: center;
}
.grow{
  flex-grow:1;
}
