/* 通用设置 */

page {
  color: #333;
  font-size: 24rpx;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0 0;
  box-sizing: border-box;
}

.relative {
  position: relative;
}

.absolute {
  position: absolute;
}

.fixed {
  position: fixed;
  z-index: 10;
}

.flex {
  display: flex;
}

.flex-wrap {
  flex-wrap: wrap;
}

.grow {
  flex-grow: 1;
}

.ff {
  background: #fff !important;
}

.greyBack {
  height: 24rpx;
  background: #f2f2f2;
}

textarea {
  height: 200rpx;
}

.w1 {
  width: 10%;
}

.w2 {
  width: 20%;
}

.w3 {
  width: 30%;
}

.w4 {
  width: 40%;
}

.w5 {
  width: 50%;
}

.w6 {
  width: 60%;
}

.w7 {
  width: 70%;
}

.w8 {
  width: 80%;
}

.w9 {
  width: 90%;
}

.w10 {
  width: 100%;
}

.auto {
  margin-left: auto !important;
  margin-right: auto !important;
}

.m1 {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.m2 {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.m3 {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}

.m4 {
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.m5 {
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}

.m6 {
  margin-top: 60rpx;
  margin-bottom: 60rpx;
}

.m7 {
  margin-top: 70rpx;
  margin-bottom: 70rpx;
}

.m8 {
  margin-top: 80rpx;
  margin-bottom: 80rpx;
}

.center {
  text-align: center;
}

.table-item {
  padding: 35rpx 0;
  color: #353535;
  margin-left: 25rpx;
  font-size: 32rpx;
  line-height: 52rpx;
}

.table-item input {
  color: #000;
}

.table-bottom {
  height: 1px;
  border-bottom: 1px solid #e2e2e2;
  position: relative;
  left: 4%;
  width: 94%;
}

.picker {
  height: 52rpx;
}

.absolute-center {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
}

.input-placeholder {
  color: #d2d2d2;
}

.goBack {
  color: #000;
  font-size: 70rpx;
  position: fixed;
  left: 25rpx;
  top: 20rpx;
  margin-top:2.5vh;
  z-index: 99;
  transform: rotateZ(90deg);
}

.noData {
  padding-top: 150rpx;
}

.noData image {
  height: 312rpx;
  width: 312rpx;
}

.noData text {
  font-size: 40rpx;
}

.noPermission>image {
  height: 280rpx;
  width: 280rpx;
}

.noPermission .absolute {
  top: 50%;
  margin-top: 140rpx;
  font-size: 36rpx;
}

.noPermission .absolute image {
  width: 40rpx;
  height: 40rpx;
}

/* icon-font */

@font-face {
  font-family: 'icomoon';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
         url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
         url('icomoon.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

/* [class^="icon-"], [class*=" icon-"] { */

/* 导入component时无法解析复杂的css选择器 */

.icon {
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  /* line-height: 1; */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-chevron-down:before {
  content: "\e900";
}

.icon-chevron-right:before {
  content: "\e901";
}

.icon-pencil:before {
  content: "\e905";
}

.icon-search:before {
  content: "\e986";
}
