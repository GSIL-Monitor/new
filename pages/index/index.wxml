<view>
  热销商品排行(月)
</view>
<view>
  本月总销量:{{statisticalData.totalSales}}
</view>
<view class="flex">
  <view wx:for="{{topSku}}" wx:key="*this.SkuId" class="w3">
    <view>
      销量:{{item.TotalPayment}}
      <image src='{{item.PicUrl}}'></image>
    </view>
  </view>
</view>


<view class="flex flex-wrap" style="justify-content:center;">
  <view wx:if="{{statisticalData.ads}}" class="w3">
    <view>{{statisticalData.ads}}</view>
    <view>广告总数</view>
  </view>
  <view wx:if="{{statisticalData.software}}" class="w3">
    <view>{{statisticalData.software}}</view>
    <view>App总数</view>
  </view>
  <view wx:if="{{statisticalData.product}}" class="w3">
    <view>{{statisticalData.product}}</view>
    <view>商品总数</view>
  </view>
  <view wx:if="{{statisticalData.coupon}}" class="w3">
    <view>{{statisticalData.coupon}}</view>
    <view>红包总数</view>
  </view>
  <view wx:if="{{statisticalData.orderCount}}" class="w3">
    <view>{{statisticalData.orderCount}}</view>
    <view>订单总数</view>
  </view>
  <view wx:if="{{statisticalData.member}}" class="w3">
    <view>{{statisticalData.member}}</view>
    <view>会员总数</view>
  </view>
</view>


<view>
  <canvas canvas-id="myCanvas" style="height:300px;width:100%"></canvas>
</view>