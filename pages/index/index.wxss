.title{
  font-size: 36rpx;
  color: #fff;
  top: 60rpx;
}

.topSkus-head {
  border-left: 12rpx solid #b31786;
  padding-left: 15rpx;
  font-size: 35rpx;
  margin-left: 30rpx;
}

.topSkus-mid view {
  height: 88rpx;
  position: relative;
}

.topSkus-mid image {
  width: 88rpx;
  height: 100%;
  margin-left: 30rpx;
}

.topSkus-mid .text1 {
  color: #666;
  font-size: 24rpx;
  margin-left: 20rpx;
}

.topSkus-mid .text2 {
  color: #ff1744;
  font-size: 44rpx;
  position: relative;
  top: 15rpx;
  margin-left: 10rpx;
}

.topSkus-body {
  border-top: 1px solid #e2e2e2;
  padding-top: 30rpx;
}

.topSkus-body .rank {
  width: 48rpx;
  height: 52rpx;
  position: absolute;
  right: -20rpx;
  top: -25rpx;
}

.topSkus-body .sales text {
  color: #aaa;
  font-size: 20rpx;
  line-height: 36rpx;
  display: inline-block;
  margin-right: 5rpx;
  position: relative;
  top: -10rpx;
}

.topSkus-body .sales view {
  color: #ff1744;
  font-size: 36rpx;
  display: inline-block;
  position: relative;
  top: -5rpx;
}

.statistics{
  margin-bottom: 75rpx;
}
.statistics>.w3 {
  /* width:auto; */
  flex-grow: 1;
  padding: 25rpx 0;
  /* outline: 1px solid #e2e2e2; */
  border-bottom: 1px solid #e2e2e2;
}

.statistics>.w3:nth-child(1), .statistics>.w3:nth-child(4) {
  border-right: 1px solid #e2e2e2;
  padding-left: 25rpx;
}
.statistics>.w3:nth-child(3), .statistics>.w3:nth-child(6) {
  border-left: 1px solid #e2e2e2;
  padding-right: 25rpx;
}

.statistics>.w3 :nth-child(2) {
  color: #ff1744;
  font-size: 36rpx;
}

.statistics image {
  width: 80rpx;
  height: 80rpx;
}

.dateSearch {
  margin-left: auto;
  margin-right: auto;
  background: #eee;
  padding: 0;
  border-radius: 10rpx;
  overflow: hidden;
}

.table-item, .picker {
  line-height: 60rpx;
}

.selected {
  color: #fff;
  background: #b31786;
}
